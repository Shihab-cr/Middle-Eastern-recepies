{
    "version": 2,
    "builds": [
        {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
        }
    ],
    "routes": [
        { "handle": "filesystem" },
        { "src": "/(.*)", "dest": "/index.html" }
    ],
    "cleanUrls": true,
    "trailingSlash": false,
    "functions": {
        "api/**/*.js": {
        "memory": 128,
        "maxDuration": 10
        }
    }
}
